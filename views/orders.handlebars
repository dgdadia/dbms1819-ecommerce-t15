<style>
.card{
  background-color: pink;
  margin-top: 100px;
  width: 50%;
}
</style>

<div class="card">
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">NO.</th>
      <th scope="col">ORDER</th>
    </tr>  
  </thead>
  <tbody>
  	{{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.name}}</td>
    {{/each}}
    </tr>
  </tbody>
</table>

  </tbody>
</table>
</div>